 "html": "{% set website_settings = frappe.get_doc(\"Website Settings\") %}\n\n<div class=\"page-break\">\n    <table>\n        <tr>\n            <td style=\"padding-left: 0!important;\">\n                <img src=\"{{ website_settings.app_logo }}\" style=\"height: 5rem; object-fit: contain;\"/>\n            </td>\n            <td>\n                {{doc.company or '-'}} <br/> \n                Gold Coast Office, Tower Eiffel Lt. 11, Jl. Pantai Indah Kapuk,Kamal Muara, Kecamatan <br/>\n                Penjaringan <br/>\n                Tel: (021) 50111290 / 292\n            </td>\n        </tr>\n    </table>\n    <h3 class=\"text-center\"  style=\"text-decoration: underline;\">KONTRAK JUAL - BELI</h3>\n    <h4 class=\"text-center\">No : {{doc.name}}</h4>\n    <table class=\"content\" style=\"width: 100%;\">\n        <tr>\n            <td>Penjual</td>\n            <td>:</td>\n            <td>{{doc.company or '-'}}</td>\n        </tr>\n        <tr>\n            <td>Pembeli</td>\n            <td>:</td>\n            <td>{{doc.customer_name or '-'}}</td>\n        </tr>\n        <tr>\n            <td>Komoditi</td>\n            <td>:</td>\n            <td>{{doc.komoditi or '-'}}</td>\n        </tr>\n        <tr>\n            <td>Kuantitas</td>\n            <td>:</td>\n            <td>{{frappe.format_value(doc.items[0].qty, \"Float\")}} {{doc.items[0].uom}}</td>\n        </tr>\n        <tr>\n            <td>Kualitas</td>\n            <td>:</td>\n            <td>\n                <table style=\"width: 15rem;\">\n                    {%- for row in doc.keterangan_per_komoditi -%}\n                    <tr>\n                        <td style=\"padding-top: 0!important;\">{{row.parameter}}</td>\n                        <td style=\"padding-top: 0!important;\">:</td>\n                        <td style=\"padding-top: 0!important;\">{{row.keterangan}} {{row.min_or_max or ''}}</td>\n                    </tr>\n                    {%- endfor -%}\n                </table>\n            </td>\n        </tr>\n        <tr>\n            <td>Harga Satuan</td> \n            <td>:</td> \n            <td>\n                {% if doc.taxes and doc.taxes|length > 0 %}\n                    {{frappe.format_value(doc.items[0].rate / (doc.taxes[0].rate / 100 + 1), \"Currency\")}}/{{doc.items[0].uom}} + PPN 12% {{frappe.format_value((doc.items[0].rate / (doc.taxes[0].rate / 100 + 1)) * (doc.taxes[0].rate / 100), \"Currency\")}} (Harga x 12% x 11/12) = {{frappe.format_value(doc.items[0].rate, \"Currency\")}}/{{doc.items[0].uom}}\n                {% endif %}\n            </td>\n        </tr>\n        <!--{%- for row in doc.taxes -%}-->\n        <!--<tr>-->\n        <!--    <td>{{row.charge_type}} {{frappe.format_value(row.rate, \"Float\")}}</td>-->\n        <!--    <td>:</td>-->\n        <!--    <td>{{frappe.format_value(row.tax_amount, \"Currency\")}}</td> -->\n        <!--</tr>-->\n        <!--{%- endfor -%}-->\n        <tr>\n            <td>Jumlah Harga</td>\n            <td>:</td>\n            <td>{{frappe.format_value(doc.grand_total, \"Currency\")}}</td>\n        </tr>\n        <tr>\n            <td>Terbilang</td>\n            <td>:</td>\n            <td class=\"text-capitalize\">{{money_in_words_idr(doc.grand_total)}}</td>\n        </tr>\n        <tr>\n            <td>Tempat Penyerahan</td>\n            <td>:</td>\n            <td>{{doc.tempat_penyerahan}}</td>\n        </tr>\n        <tr>\n            <td>Waktu Penyerahan</td>\n            <td>:</td>\n            <td>{{ frappe.utils.getdate(doc.delivery_date).strftime(\"%d %B %Y\") or '-' }} - {{ frappe.utils.getdate(doc.end_delivery_date).strftime(\"%d %B %Y\") or '-' }}</td>\n        </tr>\n        <tr>\n            <td>Syarat Pembayaran</td>\n            <td>:</td>\n            <td>\n                {% set bank = frappe.get_doc(\"Bank Account\", doc.no_rekening_tujuan) %}\n                <table>\n                    <tr>\n                        <td>Ditranfer ke</td>\n                        <td>:</td>\n                        <td>{{bank.bank}}, {{bank.branch_code}}</td>\n                    </tr>\n                    <tr>\n                        <td>A/C</td>\n                        <td>:</td>\n                        <td>{{bank.bank_account_no}}</td>\n                    </tr>\n                    <tr>\n                        <td>A/N</td>\n                        <td>:</td>\n                        <td>{{bank.company}}</td>\n                    </tr>\n                </table>\n            </td>\n        </tr>\n        <tr>\n            <td>Catatan</td>\n            <td>:</td>\n            <td>{{doc.catatan or '-'}}</td>\n        </tr>\n        <tr>\n            <td>Lainnya</td>\n            <td>:</td>\n            <td>{{doc.lainnya or '-'}}</td>\n        </tr>\n    </table>\n    <p class=\"text-right mt-4\">Jakarta, {{ frappe.utils.getdate(doc.transaction_date).strftime(\"%d %B %Y\") or '-' }}</p>\n    <table style=\"width: 100%!important; margin-top: 4rem;\">\n        <tr>\n            <td style=\"text-align: left; float: left; padding-left: 0!important;\">\n                <span style=\"display: block; margin-bottom: 5rem; white-space: nowrap;\">\n                    PENJUAL, <br/>\n                    {{doc.company or ''}}\n                </span>\n                <!--(<div class=\"ttd_line\"></div>)-->\n                <span style=\"display: block;\">\n                    <span style=\"text-decoration: underline; display: block;\">{{doc.penandatangan or '-'}}</span>\n                    {{doc.jabatan_penandatangan or '-'}}\n                </span>\n            </td>\n            <td style=\"text-align: left; float: right; padding-right: 0!important;\">\n                <span style=\"display: block; margin-bottom: 5rem; white-space: nowrap;\">\n                    PEMBELI, <br/>\n                    {{doc.customer_name or '-'}}\n                </span>\n                <!--(<div class=\"ttd_line\"></div>)-->\n                <span style=\"display: block;\">\n                    <span style=\"text-decoration: underline; display: block;\">{{(doc.customer_address or \"\").replace(\"-Billing\", \"\")}}</span>\n                    KUASA DIREKSI\n                </span>\n            </td>\n        </tr>\n    </table>\n</div>",
